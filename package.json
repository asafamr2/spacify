{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "compile-space-full": "ts-node --project space-data/tsconfig.json space-data/compile_space.ts -i space-data/raw -o public/build/spaceobject.json",
    "compile-space-schema": "typescript-json-schema space-data/schema.ts --required SpaceObject > space-data/schema.json",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "postinstall": "husky install",
    "validate": "svelte-check",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/sqlite3": "^3.1.6",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ajv": "^7.0.3",
    "alasql": "^0.6.5",
    "better-ajv-errors": "^0.7.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "eslint": "^7.17.0",
    "hammerjs": "^2.0.8",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.31.2",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.1",
    "three": "^0.124.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "typescript-json-schema": "^0.47.0",
    "vscode-json-languageservice": "^4.0.0",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
